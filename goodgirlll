int main(void){
    struct student indyy;
    strcpy(indyy.name, "indyy");
    strcpy(indyy.NRP, "5025231007");
   indyy.age = 18;

    for(int i = 0; i < 9; i++){
        indyy.grades[i] = 'A';
    }

    printfStudent(indyy);
}
